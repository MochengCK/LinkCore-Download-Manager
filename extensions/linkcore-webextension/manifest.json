{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.0.0",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "permissions": [
    "downloads",
    "storage",
    "contextMenus",
    "cookies"
  ],
  "host_permissions": [
    "http://127.0.0.1/*",
    "http://127.0.0.1:16900/*",
    "http://localhost/*",
    "http://localhost:16900/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "__MSG_actionTitle__",
    "default_popup": "popup.html"
  }
}
